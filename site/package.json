{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "eslint-plugin-vue": "^4.7.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.17",
    "xo": "^0.23.0"
  },
  "files": [
    "../cli/helpers.js"
  ],
  "xo": {
    "extensions": [
      "vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/html-indent": [
        "error",
        "tab"
      ],
      "import/extensions": [
        "error",
        "never",
        {
          "vue": "always"
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "vue$"
          ]
        }
      ],
      "unicorn/filename-case": 0
    }
  }
}
